<%- include('partials/head') %>
  
    <section class="header">
        <h1 class="display-1">Placeholder</h1>
    </section>
    <main class="main">
        <form submit="/hello" id="searchForm"><label id="summonerName"></label>
            <input type="text" id="summonername" placeholder="Summoner Name">

            <label for="lane">Lane:</label>

            <select name="pets" id="pet-select">
                <option value="">--Please choose an option--</option>
                <option value="top">Top</option>
                <option value="jg">Jungle</option>
                <option value="mid" selected="selected">Mid</option>
                <option value="bot">Bot</option>
                <option value="sup">Support</option>
            </select>

            <button>Add to Team</button>
        </form>

        <div class="row mt-5 mx-auto" >
            <h2>Team Stuff</h2>
            <div class="col-sm-4 col-lg-2">
                <div class="roleheader" id="topheader">Top</div>
                <div id="top"></div>

                <div class="images">
                </div>
            </div>

            <div class="col-sm-4 col-lg-2">
                <div class="roleheader" id="jgheader">Jungle</div>
                <div id="jg"></div>

                <div class="images">
                </div>
            </div>
            <div class="col-sm-4 col-lg-2">

                <div class="roleheader" id="midheader">Mid</div>
                <div id="mid"></div>
                
            </div>
            <div class="col-sm-4 col-lg-2">

                <div class="roleheader"  id="botheader">Bot</div>
                <div id="bot"></div>


                <div class="images">
                </div>
            </div>
            <div class="col-sm-4 col-lg-2">

                <div class="roleheader" id="supheader">Support</div>
                <div id="sup"></div>
            </div>
        </div>
        </div>
        </div>
    </main>

    <!-- needed for axios API calls -->
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <!-- Needed for personal javascript -->
    <script src="app.js"></script>
    <!-- Needed for bootstrap code -->
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
        integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js"
        integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+"
        crossorigin="anonymous"></script>
</body>

</html>